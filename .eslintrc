{
  "parserOptions": {
    "ecmaVersion": 8,
    "sourceType": "module"
  },
  "rules": {
    "accessor-pairs": 2,
    "array-bracket-spacing": 2,
    "arrow-parens": 2,
    "arrow-spacing": 2,
    "brace-style": [2, "1tbs", { "allowSingleLine": true }],
    "camelcase": [2, { "properties": "never" }],
    "curly": 2,
    "default-case": 2,
    "dot-notation": 2,
    "eol-last": 2,
    "eqeqeq": 2,
    "indent": [2, 2, { "SwitchCase": 1 }],
    "key-spacing": 2,
    "keyword-spacing": 2,
    "max-len": [2, 120],
    "no-extend-native": 2,
    "no-mixed-spaces-and-tabs": 2,
    "no-multi-str": 2,
    "no-multiple-empty-lines": 2,
    "no-spaced-func": 2,
    "no-trailing-spaces": 2,
    "no-unused-vars": 2,
    "no-with": 2,
    "operator-linebreak": 2,
    "quotes": [2, "single"],
    "semi": 2,
    "space-before-blocks": 2,
    "space-before-function-paren": [2, { "named": "never" }],
    "space-in-parens": 2,
    "space-infix-ops": 2,
    "space-unary-ops": 2,
    "valid-jsdoc": 2
  }
}
